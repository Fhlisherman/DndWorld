                "command": "npx vite --help",
                "example": {
                  "options": {
                    "port": 3000
                  }
                }
              }
            },
            "executor": "nx:run-commands",
            "configurations": {},
            "parallelism": true
          },
          "preview": {
            "dependsOn": [
              "build"
            ],
            "options": {
              "cwd": "apps/DndWorld",
              "command": "vite preview"
            },
            "metadata": {
              "technologies": [
                "vite"
              ],
